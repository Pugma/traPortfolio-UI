    "clean": "rimraf scripts/traPorfolio.v1.yaml src/lib/apis/generated",
    "postinstall": "npm run fetch-openapi && npm run gen-api"
  },
  "dependencies": {
    "@iconify/iconify": "^1.0.7",
    "axios": "^0.21.1",
    "pinia": "^2.0.28",
    "ress": "^5.0.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.277",
    "@openapitools/openapi-generator-cli": "^2.1.15",
    "@stoplight/prism-cli": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.4.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "ts-morph": "^17.0.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-purge-icons": "^0.9.2"
  },
  "private": true
}
