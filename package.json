    "postinstall": "npm run fetch-openapi && npm run gen-api"
  },
  "dependencies": {
    "@iconify/iconify": "^1.0.7",
    "axios": "^0.21.0",
    "direct-vuex": "^0.12.0",
    "ress": "^3.0.0",
    "vue": "^3.0.3",
    "vue-router": "^4.0.0-rc.6",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.268",
    "@openapitools/openapi-generator-cli": "^2.1.7",
    "@stoplight/prism-cli": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@vue/compiler-sfc": "^3.0.3",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-vue": "^7.1.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "ts-morph": "^9.1.0",
    "typescript": "^4.1.2",
    "vite": "^1.0.0-rc.13",
    "vite-plugin-purge-icons": "^0.4.5"
  },
  "private": true
